<h1>Register</h1>
 
<% form_for @user, :html => { :multipart => true }, :url => account_path do |f| %>
  <%= f.error_messages %>
  <%= render :partial => "form", :object => f %>
  <%= f.label :email %><br />
  <%= f.text_field :email %><br />
  <%= f.label :photo %><br />
	<%= f.file_field :photo %>
	<br />
  <%= f.submit "Register" %>
<% end %>
